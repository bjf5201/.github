{
  "name": "github-config",
  "author": "Bethany Fannin <contact@bethanyfannin.com>",
  "version": "1.0.0",
  "description": "GitHub configuration files",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc -p tsconfig.browser.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com:bjf5201/.github.git"
  },
  "bugs": {
    "url": "https://github.com/bjf5201/.github/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@app": "./src/index.tsx",
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "export": {
        "@app": "./src/index.tsx",
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": "false",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "packageManager": "pnpm@10.6.0",
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@tailwindcss/vite": "4.0.14",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "@typescript-eslint/parser": "8.26.1",
    "@vitejs/plugin-react-swc": "3.8.0",
    "@vitest/eslint-plugin": "1.1.37",
    "autoprefixer": "10.4.21",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.0.0",
    "jsdom": "26.0.0",
    "lint-staged": "15.5.0",
    "mkdirp": "3.0.1",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vite": "6.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  }
}